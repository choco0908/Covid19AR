<script type="text/javaScript" language="javascript">

function fn_search(pageNo){
	$("#pageIndex").val(pageNo);
	fn_submit();
}

function fn_submit(){
	var frm = $("#form1");
	frm.attr("action", "/bdBoardList.do;jsessionid=1qoIOSSbTif8eGpXcpHG1lcs9aJoeW5ZU92H4CMPpNqn8Uez4TUHTNsTRaWFa1pm.mohwwas1_servlet_engine40");
	frm.submit();
}

// 넷퍼넬  code
$(document).ready(function() {
	NetFunnel_Complete();
});
</script>

